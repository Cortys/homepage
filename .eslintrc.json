{
	"root": true,
	"extends": "cortys",
	"parserOptions": {
		"ecmaVersion": 2020
	},
	"env": {
		"browser": true,
		"node": false
	},
	"globals": {
		"process": false,
		"__dirname": false,
		"__filename": false
	},
	"overrides": [{
		"files": ["src/**/*.js"],
		"parserOptions": {
			"sourceType": "module"
		},
		"rules": {
			"import/no-commonjs": "error",
			"no-underscore-dangle": "off"
		}
	}],
	"rules": {
		"import/no-cycle": "off"
	}
}
